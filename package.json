{
    "name": "string-converter",
    "displayName": "Text Manipulator",
    "version": "0.1.2",
    "icon": "images/icon.png",
    "description": "Easily convert strings in your code with functions like camelCase, toUpper, replace, and many more.",
    "publisher": "adamwalzer",
    "author": {
        "name": "Adam Walzer"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/adamwalzer/string-converter"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Formatters"
    ],
    "license": "MIT",
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "showTextManipulatorMenu",
                "title": "menu",
                "category": "Text Manipulator"
            },
            {
                "command": "camelCase",
                "title": "camelCase          (`This is exämple tęxt` => `thisIsExampleText`)",
                "category": "Text Manipulator"
            },
            {
                "command": "capitalize",
                "title": "capitalize          (`This is exämple tęxt` => `This is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "constantCase",
                "title": "constantCase          (`This is exämple tęxt` => `THIS_IS_EXAMPLE_TEXT`)",
                "category": "Text Manipulator"
            },
            {
                "command": "deburr",
                "title": "deburr          (`This is exämple tęxt` => `This is example text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "dotCase",
                "title": "dotCase          (`This is exämple tęxt` => `this.is.example.text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "escape",
                "title": "escape          (`This is ex&mple tęxt` => `This is ex&amp;mple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "escapeRegExp",
                "title": "escapeRegExp          (`[This is exämple tęxt]` => `\\[This is exämple tęxt\\]`)",
                "category": "Text Manipulator"
            },
            {
                "command": "kebabCase",
                "title": "kebabCase          (`This is exämple tęxt` => `this-is-example-text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "lowerCase",
                "title": "lowerCase          (`This is exämple tęxt` => `this is example text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "lowerFirst",
                "title": "lowerFirst          (`This is exämple tęxt` => `this is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "pad",
                "title": "pad          (`This is exämple tęxt`, 24, ~ => `~~This is exämple tęxt~~`)",
                "category": "Text Manipulator"
            },
            {
                "command": "padEnd",
                "title": "padEnd          (`This is exämple tęxt`, 2, ~ => `This is exämple tęxt~~~~`)",
                "category": "Text Manipulator"
            },
            {
                "command": "padStart",
                "title": "padStart          (`This is exämple tęxt`, 24, ~ => `~~~~This is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "parseInt",
                "title": "parseInt          (`11a` => `11`)",
                "category": "Text Manipulator"
            },
            {
                "command": "pascalCase",
                "title": "pascalCase          (`This is exämple tęxt` => `ThisIsExampleText`)",
                "category": "Text Manipulator"
            },
            {
                "command": "pathCase",
                "title": "pathCase          (`This is exämple tęxt` => `this/is/example/text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "repeat",
                "title": "repeat          (`tęxt`, 3 => `tęxttęxttęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "replace",
                "title": "replace          (`This is exämple tęxt`, This, this => `this is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "snakeCase",
                "title": "snakeCase          (`This is exämple tęxt` => `this_is_example_text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "spaceCase",
                "title": "spaceCase          (`thisIsExampleText` => `this is example text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "split",
                "title": "split          (`This is exämple tęxt`, e => `This is ,xämpl, tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "startCase",
                "title": "startCase          (`This is exämple tęxt` => `This Is Example Text`)",
                "category": "Text Manipulator"
            },
            {
                "command": "toLower",
                "title": "toLower          (`This is exämple tęxt` => `this is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "toUpper",
                "title": "toUpper          (`This is exämple tęxt` => `THIS IS EXÄMPLE TĘXT`)",
                "category": "Text Manipulator"
            },
            {
                "command": "trim",
                "title": "trim          (`  This is exämple tęxt  ` => `This is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "trimEnd",
                "title": "trimEnd          (`  This is exämple tęxt  ` => `  This is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "trimStart",
                "title": "trimStart          (`  This is exämple tęxt  ` => `This is exämple tęxt  `)",
                "category": "Text Manipulator"
            },
            {
                "command": "unescape",
                "title": "unescape          (`This is ex&amp;mple tęxt` => `This is ex&mple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "upperCase",
                "title": "upperCase          (`This is exämple tęxt` => `THIS IS EXAMPLE TEXT`)",
                "category": "Text Manipulator"
            },
            {
                "command": "upperFirst",
                "title": "upperFirst          (`this is exämple tęxt` => `This is exämple tęxt`)",
                "category": "Text Manipulator"
            },
            {
                "command": "words",
                "title": "words          (`This is exämple text` => `This,is,exämple,text`)",
                "category": "Text Manipulator"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "lodash": "^4.17.13"
    }
}
